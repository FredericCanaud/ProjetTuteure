{
  "connection": "default",
  "collectionName": "restaurants",
  "info": {
    "name": "association",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "nom": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "personnes": {
      "collection": "personne",
      "via": "assocation"
    },
    "horaireOuverture": {
      "type": "date"
    },
    "horaireFermeture": {
      "type": "date"
    },
    "description": {
      "type": "richtext"
    },
    "adressePostale": {
      "type": "richtext"
    },
    "adresseEmail": {
      "type": "email"
    },
    "telephone": {
      "type": "integer"
    },
    "lieus": {
      "collection": "lieu",
      "via": "association"
    }
  }
}